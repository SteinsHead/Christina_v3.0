<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.christina_v30">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"


        android:icon="@drawable/wife2"
        android:roundIcon="@drawable/wife2"


        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        android:usesCleartextTraffic="true"
        >
        <activity android:name=".SignUpCenter"></activity>
        <activity android:name=".SearchCenter"></activity>
        <activity android:name=".LoginCenter"></activity>
        <activity android:name=".UserCenter"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>